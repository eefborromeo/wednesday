<h1>User#edit</h1>
<p>Find me in app/views/user/edit.html.erb</p>

<%= form_with model: @user do |form| %>
  <div>
    <%= form.label "Email" %>
    <br>
    <%= form.email_field :email %>
  </div>

  <div>
    <%= form.label "Password" %>
    <br>
    <%= form.password_field :password %>
  </div>

  <div>
    <%= form.submit %>
  </div>
<% end %>